{
  "name": "hawkinit",
  "version": "1.9.1",
  "description": "This CLI tool sets up the Hawkular Services together with couple of servers to monitor.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hawkular/hawkinit"
  },
  "scripts": {
    "test": "mocha specs/*",
    "start": "node ./index.js",
    "lint": "[ -f ./node_modules/.bin/eslint ] && ./node_modules/.bin/eslint index.js lib/**.js specs",
    "prepublish": "npm run test",
    "pretest": "npm run lint"
  },
  "bin": {
    "hawkinit": "./index.js"
  },
  "keywords": [
    "hawkular",
    "docker",
    "docker-compose",
    "cli",
    "cassandra"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">= 4.3.2"
  },
  "author": "Jiri Kremser",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^1.1.3",
    "clui": "^0.3.1",
    "cmd-exists-sync": "^0.1.0",
    "command-exists": "^1.0.2",
    "command-line-args": "^3.0.4",
    "command-line-usage": "^3.0.8",
    "figlet": "^1.2.0",
    "inflected": "^2.0.1",
    "inquirer": "^1.2.3",
    "js-yaml": "^3.8.3",
    "lodash": "^4.17.2",
    "mustache": "^2.3.0",
    "node-rest-client": "^3.1.0",
    "object-hash": "^1.1.8",
    "object-path": "^0.11.4",
    "parse-key-value": "^1.0.0",
    "tmp": "0.0.31"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-chai-friendly": "^0.3.6",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2"
  }
}
